{
  "name": "vanilla-context",
  "version": "1.0.12",
  "description": "Pure javascript context menu library",
  "main": "dist/Container.js",
  "types": "dist/Container.d.ts",
  "author": "winetree94",
  "license": "MIT",
  "homepage": "https://winetree94.github.io/VanillaContextMenu",
  "repository": "https://github.com/winetree94/VanillaContextMenu",
  "scripts": {
    "debug:context": "webpack-dev-server --config webpack.dev.js",
    "build:context": "tsc && webpack --config webpack.prod.js -p && copy:docs",
    "copy:docs": "rm -rf docs && mkdir docs && cp ./dist/index.html ./docs/index.html && cp ./dist/vanilla-context.min.js ./docs/vanilla-context.min.js && cp ./dist/vanilla-context.min.css ./docs/vanilla-context.min.css",
    "test:lint": "eslint \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@babel/core": "^7.8.7",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^2.3.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}
